//number of 1's and 0's in an array
#include<stdio.h>
int main()
{
    int arr[20] , n , count_0=0 , count_1=0;
    printf("Enter the size of array:- ");
    scanf("%d" , &n);
    printf("Enter array elements(only 0 and 1):-\n");
    for (int i = 0; i < n; i++)
    {
        scanf("%d" , &arr[i]);
    }
    for (int i = 0; i < n; i++)
    {
        if(arr[i]==0)
        count_0++;
        else if(arr[i]==1)
        count_1++;
        else
        goto end;
    }
    printf("The number of 1's in the entered string is %d\n" , count_1);
    printf("The number of 0's in the entered string is %d\n" , count_0);
    end:
    printf("Invalid input!!");
    return 0;
}
